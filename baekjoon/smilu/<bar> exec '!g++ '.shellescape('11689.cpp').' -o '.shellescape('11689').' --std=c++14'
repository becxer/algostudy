#include<iostream>
using namespace std;

typedef signed long long lld;

int main()
{
    lld n;
    scanf("%lld", &n);
    lld cnt = 1;
    lld tmp = n;
    for(lld i=2; tmp > 1; ++i) {
        lld cnt2 = 0;
        while(tmp%i == 0) {
            tmp /= i;
            ++cnt2;
        }
        cnt *= cnt2 + 1;
    }
    printf("%lld", n-cnt+1);
}
